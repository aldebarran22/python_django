<?xml version="1.0" encoding="UTF-8"?>
<libros>
	{% for libro in libros %}
	<libro>
		<isbn>
			{{libro.isbn}}
		</isbn>
		<titulo>
			{{libro.name}}
		</titulo>
		<precio>
			{{libro.price}}
		</precio>
		<editor>
			{{libro.publisher}}
		</editor>
	</libro>
	{% endfor %}
</libros>
